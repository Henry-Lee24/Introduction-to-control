<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE params SYSTEM "params.dtd">


<params name="CyberFollow">
  <section name="Header">
    <attstr name="name" val="CyberFollow"/>
    <attstr name="description" val="Follow Experiment with CyberTORCS"/>
    <attnum name="priority" val="200"/>
    <attstr name="menu image" val="data/img/splash-qr.png"/>
  </section>

  <section name="Tracks">
    <attnum name="maximum number" val="1"/>
    <section name="1">
      <attstr name="name" val="michigan"/>
      <attstr name="category" val="oval"/>
    </section>

  </section>

  <section name="Races">
    <section name="1">
      <attstr name="name" val="CyberFollow"/>
    </section>

  </section>

  <section name="CyberFollow">
    <attnum name="distance" unit="km" val="0"/>
    <attstr name="type" val="race"/>
    <attstr name="starting order" val="drivers list"/>
    <attstr name="restart" val="yes"/>
    <attnum name="laps" val="2"/>
    <section name="Starting Grid">
      <attnum name="rows" val="1"/>
      <attnum name="distance to start" val="25"/>
      <attnum name="distance between columns" val="20"/>
      <attnum name="offset within a column" val="10"/>
      <attnum name="initial speed" val="0"/>
      <attnum name="initial height" val="0.2"/>
    </section>

  </section>

  <section name="Drivers">
    <attnum name="maximum number" val="40"/>
    <attstr name="focused module" val="cyberfollow"/>
    <attnum name="focused idx" val="0"/>
    
    <section name="1">
      <attnum name="idx" val="1"/>
      <attstr name="module" val="cyberfollow"/>
    </section>
    
    <section name="2">
      <attnum name="idx" val="0"/>
      <attstr name="module" val="cyberfollow"/>
    </section>
  </section>

  <section name="Configuration">
    <attnum name="current configuration" val="1"/>
 
  </section>

  <section name="Drivers Start List">

    <section name="1">
      <attnum name="idx" val="1"/>
      <attstr name="module" val="cyberfollow"/>
    </section>

    <section name="2">
      <attnum name="idx" val="0"/>
      <attstr name="module" val="cyberfollow"/>
    </section>
  </section>

</params>
